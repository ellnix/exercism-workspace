#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/home/fourhundredlux/Code/github/ellnix/exercism-workspace/wasm/hello-world/node_modules/.pnpm/@babel+node@7.23.9_@babel+core@7.24.4/node_modules/@babel/node/bin/node_modules:/home/fourhundredlux/Code/github/ellnix/exercism-workspace/wasm/hello-world/node_modules/.pnpm/@babel+node@7.23.9_@babel+core@7.24.4/node_modules/@babel/node/node_modules:/home/fourhundredlux/Code/github/ellnix/exercism-workspace/wasm/hello-world/node_modules/.pnpm/@babel+node@7.23.9_@babel+core@7.24.4/node_modules/@babel/node_modules:/home/fourhundredlux/Code/github/ellnix/exercism-workspace/wasm/hello-world/node_modules/.pnpm/@babel+node@7.23.9_@babel+core@7.24.4/node_modules:/home/fourhundredlux/Code/github/ellnix/exercism-workspace/wasm/hello-world/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/home/fourhundredlux/Code/github/ellnix/exercism-workspace/wasm/hello-world/node_modules/.pnpm/@babel+node@7.23.9_@babel+core@7.24.4/node_modules/@babel/node/bin/node_modules:/home/fourhundredlux/Code/github/ellnix/exercism-workspace/wasm/hello-world/node_modules/.pnpm/@babel+node@7.23.9_@babel+core@7.24.4/node_modules/@babel/node/node_modules:/home/fourhundredlux/Code/github/ellnix/exercism-workspace/wasm/hello-world/node_modules/.pnpm/@babel+node@7.23.9_@babel+core@7.24.4/node_modules/@babel/node_modules:/home/fourhundredlux/Code/github/ellnix/exercism-workspace/wasm/hello-world/node_modules/.pnpm/@babel+node@7.23.9_@babel+core@7.24.4/node_modules:/home/fourhundredlux/Code/github/ellnix/exercism-workspace/wasm/hello-world/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../../../../../../@babel+node@7.23.9_@babel+core@7.24.4/node_modules/@babel/node/bin/babel-node.js" "$@"
else
  exec node  "$basedir/../../../../../../@babel+node@7.23.9_@babel+core@7.24.4/node_modules/@babel/node/bin/babel-node.js" "$@"
fi
